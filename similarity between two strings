str1 = input("Enter String 1:\n")
str2 = input("Enter String 2:\n")

if len(str1) > len(str2):
    long = len(str1)
    short = len(str2)
else:
    long = len(str2)
    short = len(str1)
    
matchCnt = 0

for i in range (short):
    if str1[i] == str2[i]:
        matchCnt +=1
        
print("Similarity between the two said strings:")
print(matchCnt/long)
    
